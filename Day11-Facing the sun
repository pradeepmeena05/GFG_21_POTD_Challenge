
class Solution {
    // Returns count buildings that can see sunlight
    public int countBuildings(int[] height) {
        // code here
        int n=height.length;
         int count=0,max=0;
        for(int i=0;i<n;i++)
        {
            if(height[i]>max)
            {
                max=height[i];
                count++;
            }
        }
        return count ;
    }
}
